    "@google/cloud-trace": "^0.6.0",
    "body-parser": "^1.16.0",
    "config": "^1.25.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.14.1",
    "express-session": "^1.15.0",
    "googleapis": "^16.1.0",
    "zone.js": "^0.7.2",
    "jade": "^1.11.0",
    "lodash": "^4.14.2",
    "nconf": "^0.8.4",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "mocha": "^3.0.2",
    "nodejs-repo-tools": "git+https://git@github.com/GoogleCloudPlatform/nodejs-repo-tools.git",
